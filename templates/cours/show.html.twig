{% extends 'base_BACK.html.twig' %}


{% block title %}Cours
{% endblock %}

{% block body %}
	<h1>Cours</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ cour.id }}</td>
			</tr>
			<tr>
				<th>Nom</th>
				<td>{{ cour.nom }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ cour.description }}</td>
			</tr>
			<tr>
				<th>Note</th>
				<td>{{ cour.note }}</td>
			</tr>
			<tr>
				<th>Video</th>
				<td>{{ cour.video }}</td>
			</tr>
			<tr>
				<th>Pdf</th>
				<td>{{ cour.pdf }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_cours_index') }}">back to list</a>

	<a href="{{ path('app_cours_edit', {'id': cour.id}) }}">edit</a>

	{{ include('cours/_delete_form.html.twig') }}
{% endblock %}
